Folder: mygym_pro
    File: app.js
        Code:
        /* ==========================================================================
           app.js â€” UPPER/LOWER Tracker (RTL)
           ========================================================================== */
        
        // â”€â”€â”€ Section 1: Data Import (from program-data.js) â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        "use strict";
        const { FEEL, program } = window.ProgramData || {};
        if (!program) console.error("Program data missing â€” make sure program-data.js is loaded before app.js");
        // â”€â”€â”€ End Section 1 â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        
        
        
        // â”€â”€â”€ Section 2: DB Operations (localStorage) â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        const KEY = "ul_tracker_state_v2_no_video";
        const state = JSON.parse(localStorage.getItem(KEY) || "{}");
        
        function saveState(){ localStorage.setItem(KEY, JSON.stringify(state)); }
        function ensureExerciseState(exId){
          if(!state[exId]) state[exId] = { active:null, logs:[] };
          return state[exId];
        }
        // â”€â”€â”€ End Section 2 â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        
        
        
        // â”€â”€â”€ Section 3: DOM Handles & Global UI State â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        const dayNav      = document.getElementById("dayNav");
        const dayTitle    = document.getElementById("dayTitle");
        const muscles     = document.getElementById("muscles");
        const exercisesEl = document.getElementById("exercises");
        const prevBtn     = document.getElementById("prevBtn");
        const nextBtn     = document.getElementById("nextBtn");
        
        // Off-canvas (burger) elements
        const sidebar = document.getElementById("sidebar");
        const menuBtn = document.getElementById("menuBtn");
        const scrim   = document.getElementById("scrim");
        
        let currentDayIdx = 0;
        // â”€â”€â”€ End Section 3 â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        
        
        
        // â”€â”€â”€ Section 4: Mobile Off-Canvas Menu â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        function openMenu(){
          if(!sidebar || !menuBtn || !scrim) return;
          sidebar.classList.add("is-open");
          sidebar.setAttribute("aria-hidden","false");
          menuBtn.setAttribute("aria-expanded","true");
          scrim.hidden = false;
          document.body.classList.add("no-scroll");
        }
        function closeMenu(){
          if(!sidebar || !menuBtn || !scrim) return;
          sidebar.classList.remove("is-open");
          sidebar.setAttribute("aria-hidden","true");
          menuBtn.setAttribute("aria-expanded","false");
          scrim.hidden = true;
          document.body.classList.remove("no-scroll");
        }
        function toggleMenu(){
          if(!sidebar) return;
          sidebar.classList.contains("is-open") ? closeMenu() : openMenu();
        }
        menuBtn && menuBtn.addEventListener("click", toggleMenu);
        scrim   && scrim.addEventListener("click", closeMenu);
        window.addEventListener("keydown", (e)=>{ if(e.key==="Escape") closeMenu(); });
        window.addEventListener("resize", ()=>{ if(window.innerWidth > 1024) closeMenu(); });
        // â”€â”€â”€ End Section 4 â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        
        
        
        // â”€â”€â”€ Section 5: Render â€“ Top Day Navigation â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        function renderNav(){
          dayNav.innerHTML = "";
          program.forEach((d,i)=>{
            const btn = document.createElement("button");
            btn.setAttribute("role","tab");
            btn.setAttribute("aria-selected", i===currentDayIdx ? "true" : "false");
            btn.innerHTML = `<div style="font-weight:700">${d.dayLabel}</div><small>${d.muscles}</small>`;
            btn.className = i===currentDayIdx ? "active" : "";
            btn.onclick = ()=>{
              currentDayIdx = i;
              renderDay();
              renderNav();
              closeMenu(); // close off-canvas after choosing a day
            };
            dayNav.appendChild(btn);
          });
        }
        // â”€â”€â”€ End Section 5 â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        
        
        
        // â”€â”€â”€ Section 6: Render â€“ Day View (cards, alternatives, logs, chart) â”€â”€â”€â”€â”€â”€â”€
        function renderDay(){
          const day = program[currentDayIdx];
          dayTitle.textContent = day.dayLabel;
          muscles.textContent  = day.muscles;
          exercisesEl.innerHTML = "";
        
          day.exercises.forEach(ex=>{
            const exState = ensureExerciseState(ex.id);
            const active  = exState.active || ex.active;
        
            const card = document.createElement("div");
            card.className = "card";
        
            const head = document.createElement("div");
            head.className = "card-head";
            head.innerHTML = `
              <h3 class="ex-title">${ex.title}</h3>
              <div class="meta">
                <span class="tag">×¡×˜×™×Ã—×—×–×¨×•×ª/××©×š: <b>${ex.setsReps}</b></span>
                <span class="tag">×ª×¨×’×™×œ ×¤×¢×™×œ: <b>${active}</b></span>
                <span class="tag">×”×¨×’×©×”: ${ex.feel || "×‘×§×¨×” ×¢×œ ×˜×›× ×™×§×” ×•× ×©×™××”"}</span>
                <span class="tag">×”×ª×§×“××•×ª: ${ex.progress || "×¢×œ×” × ×¤×—/×§×•×©×™ ×›×©×”×¡×˜×™× ×™×¦×™×‘×™×"}</span>
              </div>
            `;
            card.appendChild(head);
        
            const grid = document.createElement("div");
            grid.className = "grid";
        
            // Alternatives
            const altSec = document.createElement("div");
            altSec.className = "section";
            altSec.innerHTML = `<h4>××œ×˜×¨× ×˜×™×‘×•×ª ×•×‘×—×™×¨×ª ×ª×¨×’×™×œ</h4>`;
        
            const alts = document.createElement("div");
            alts.className = "alts";
        
            ex.alts.forEach(alt=>{
              const row = document.createElement("div");
              row.className = "alt";
              row.innerHTML = `
                <div>
                  <div class="name">${alt.name}</div>
                  <div class="kind">${alt.kind}</div>
                </div>
                <div style="display:flex; gap:8px; align-items:center; flex-wrap:wrap">
                  <a class="btn ghost" href="https://www.youtube.com/results?search_query=${encodeURIComponent(alt.yt)}" target="_blank" rel="noopener">×™×•×˜×™×•×‘</a>
                  <button class="btn use ${active===alt.name?'primary':''}">×”×©×ª××©</button>
                </div>
              `;
              row.querySelector(".use").onclick = ()=>{
                exState.active = alt.name;
                saveState();
                renderDay();
              };
              alts.appendChild(row);
            });
        
            const feelBox = document.createElement("div");
            feelBox.className = "feel";
            feelBox.textContent = "××” ×œ×”×¨×’×™×©: " + (ex.feel || "") + (ex.progress ? " â€¢ ××ª×™ ×œ×¢×œ×•×ª: " + ex.progress : "");
            altSec.appendChild(alts);
            altSec.appendChild(feelBox);
            grid.appendChild(altSec);
        
            // Logger + chart
            const logSec = document.createElement("div");
            logSec.className = "section";
            logSec.innerHTML = `<h4>×œ×•×’ ××©×§×œ×™×/××©×š + ×’×¨×£ ×”×ª×§×“××•×ª</h4>`;
        
            const logRow = document.createElement("div");
            logRow.className = "log-row";
            logRow.innerHTML = `
              <input type="date" id="date-${ex.id}" />
              <input type="number" id="w-${ex.id}" placeholder="××©×§×œ (×§×´×’) / ×¢×•×¦××”" step="0.5" />
              <input type="number" id="r-${ex.id}" placeholder="×—×–×¨×•×ª / ×–××Ÿ (×©×³)" step="1" />
              <button class="btn primary" id="add-${ex.id}">×”×•×¡×£ ×œ×•×’</button>
            `;
            logSec.appendChild(logRow);
        
            const hint = document.createElement("div");
            hint.className = "hint";
            hint.textContent = "×”×’×¨×£ ××¦×™×’ ×”×¢×¨×›×ª 1RM (Epley) ×›××©×¨ ×™×© ××©×§×œ+×—×–×¨×•×ª. ×‘××™×¨×•×‘×™, × ×™×ª×Ÿ ×œ×”×–×™×Ÿ ×–××Ÿ/×¢×•×¦××” ×œ××¢×§×‘ ××’××”.";
            logSec.appendChild(hint);
        
            const canvas = document.createElement("canvas");
            canvas.id = "chart-" + ex.id;
            canvas.height = 140;
            logSec.appendChild(canvas);
        
            const tableWrap = document.createElement("div");
            tableWrap.className = "table-wrap";
            tableWrap.style.marginTop = "10px";
            tableWrap.innerHTML = `
              <table class="logs-table">
                <thead><tr><th>×ª××¨×™×š</th><th>××©×§×œ/×¢×•×¦××”</th><th>×—×–×¨×•×ª/×–××Ÿ</th><th>1RM ××©×•×¢×¨*</th></tr></thead>
                <tbody id="tb-${ex.id}"></tbody>
              </table>
              <div class="hint">* ×¢×‘×•×¨ ××™×¨×•×‘×™ ×”×¢×¨×š ×œ× ×™×©×™× â€” × ×™×ª×Ÿ ×œ×”×©××™×¨ ×¨×™×§ ××• 0.</div>
            `;
            logSec.appendChild(tableWrap);
        
            grid.appendChild(logSec);
            card.appendChild(grid);
            exercisesEl.appendChild(card);
        
            // Per-exercise helpers
            const dateEl = logRow.querySelector("#date-"+ex.id);
            const wEl    = logRow.querySelector("#w-"+ex.id);
            const rEl    = logRow.querySelector("#r-"+ex.id);
            const addBtn = logRow.querySelector("#add-"+ex.id);
            const tb     = tableWrap.querySelector("#tb-"+ex.id);
        
            if(!dateEl.value){
              const d = new Date();
              dateEl.value = d.toISOString().slice(0,10);
            }
        
            function epley1RM(w,r){
              const W = parseFloat(w); const R = parseInt(r,10);
              if (isNaN(W) || isNaN(R) || W<=0 || R<=0) return "";
              return Math.round((W * (1 + R/30)) * 10) / 10;
            }
        
            function renderLogs(){
              const logs = (state[ex.id]?.logs || []).slice().sort((a,b)=>a.date.localeCompare(b.date));
              tb.innerHTML = logs.map(l=>{
                const est = epley1RM(l.weight,l.reps);
                return `<tr><td>${l.date}</td><td>${l.weight ?? ""}</td><td>${l.reps ?? ""}</td><td>${est===""?"-":est}</td></tr>`;
              }).join("") || `<tr><td colspan="4" style="color:var(--muted)">××™×Ÿ ×¢×“×™×™×Ÿ ×œ×•×’×™×</td></tr>`;
        
              const ctx = canvas.getContext('2d');
              const labels = logs.map(l=>l.date);
              const data   = logs.map(l=>epley1RM(l.weight,l.reps) || null);
        
              if(canvas._chart) canvas._chart.destroy();
              canvas._chart = new Chart(ctx,{
                type:'line',
                data:{ labels, datasets:[{ label:'1RM ××©×•×¢×¨ (×× ×™×©)', data, tension:.25, pointRadius:3 }] },
                options:{
                  responsive:true,
                  maintainAspectRatio:false,
                  plugins:{ legend:{ labels:{ color:"#cfe4ff" } } },
                  scales:{
                    x:{ ticks:{ color:"#a6bfd6" }, grid:{ color:"#223243" } },
                    y:{ ticks:{ color:"#a6bfd6" }, grid:{ color:"#223243" } }
                  }
                }
              });
            }
        
            addBtn.onclick = ()=>{
              const date   = dateEl.value;
              const weight = (wEl.value===""?null:parseFloat(wEl.value));
              const reps   = (rEl.value===""?null:parseInt(rEl.value,10));
              if(!date){ alert("×× × ××œ× ×ª××¨×™×š."); return; }
              if(weight===null && reps===null){ alert("×”×–×Ÿ ×œ×¤×—×•×ª ××©×§×œ/×¢×•×¦××” ××• ×—×–×¨×•×ª/×–××Ÿ."); return; }
              const st = ensureExerciseState(ex.id);
              st.logs.push({date, weight, reps});
              saveState();
              renderLogs();
              rEl.value="";
            };
        
            renderLogs();
          });
        
          prevBtn.disabled = currentDayIdx===0;
          nextBtn.disabled = currentDayIdx===program.length-1;
        }
        // â”€â”€â”€ End Section 6 â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        
        
        
        // â”€â”€â”€ Section 7: Wiring & Init â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
        prevBtn.onclick = ()=>{ if(currentDayIdx>0){ currentDayIdx--; renderDay(); renderNav(); } };
        nextBtn.onclick = ()=>{ if(currentDayIdx<program.length-1){ currentDayIdx++; renderDay(); renderNav(); } };
        
        renderNav();
        renderDay();
        
        document.addEventListener('keydown', (e)=>{
          if (e.key === 'ArrowLeft') nextBtn.click();
          if (e.key === 'ArrowRight') prevBtn.click();
        });
        // â”€â”€â”€ End Section 7 â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€
